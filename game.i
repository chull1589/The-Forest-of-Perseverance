void updatePlayer() {





    if (player.inv && invtimer < 10) {
        invtimer++;

    }

    if (invtimer >= 10) {
        player.inv = 0;
        invtimer = 0;
    }

    timer++;

    unsigned char* collisionmap = (unsigned char *) collisionmap1Bitmap;

    if (collisionmap[((player.worldRow + player.height - 1 - (int) round(player.velocity * 0.1))*(240)+(player.worldCol))
                                                                     ]
        && collisionmap[((player.worldRow + player.height - 1 - (int) round(player.velocity * 0.1))*(240)+(player.worldCol + player.width - 1))
                                                                     ]

        && collisionmap[((player.worldRow + player.height/2 - 1 - (int) round(player.velocity * 0.1))*(240)+(player.worldCol + player.width - 1))
                                                                     ]
        ) {


        if (player.velocity > -50)
            player.velocity = (int) player.velocity + (-32 * 0.1);


        canJump = 0;


        if (collisionmap[((player.worldRow + player.height - 1 - (int) round(player.velocity * 0.1))*(240)+(player.worldCol))
                                                                         ]
            && collisionmap[((player.worldRow + player.height - 1 - (int) round(player.velocity * 0.1))*(240)+(player.worldCol + player.width - 1))
                                                                          ]
            && collisionmap[((player.worldRow + player.height - 1 - (int) round(player.velocity * 0.1))*(240)+(player.worldCol + player.width / 2 - 1))
                                                                          ]) {

            player.worldRow = (int) player.worldRow - round(player.velocity * 0.1);







        }


    } else if (!(player.worldRow > 0
            && (collisionmap[((player.worldRow - player.rdel)*(240)+(player.worldCol))]
            && collisionmap[((player.worldRow - player.rdel)*(240)+(player.worldCol + player.width - 1))]

            && collisionmap[((player.worldRow - player.rdel)*(240)+(player.worldCol + player.width/2 - 1))]
        ))) {

            canJump = 0;
    } else {

        player.velocity = 0;
        canJump = 1;
    }



    if (!(player.worldRow > 0
            && (collisionmap[((player.worldRow - player.rdel)*(240)+(player.worldCol))]
            && collisionmap[((player.worldRow - player.rdel)*(240)+(player.worldCol + player.width - 1))]

            && collisionmap[((player.worldRow - player.rdel)*(240)+(player.worldCol + player.width/2 - 1))]
        ))) {

            player.velocity = 0;
            player.worldRow++;
    }

    if ((~((*(volatile unsigned short *)0x04000130)) & ((1<<5)))) {
        if (player.worldCol - 1 >= 0
            && collisionmap[((player.worldRow - (int) round(player.velocity * 0.1))*(240)+(player.worldCol - 1))
                                                                         ]
            && collisionmap[((player.worldRow + player.height - 2 - (int) round(player.velocity * 0.1))*(240)+(player.worldCol - 1))
                                                                         ]
            && collisionmap[((player.worldRow + player.height / 2 - 1 - (int) round(player.velocity * 0.1))*(240)+(player.worldCol - 1))
                                                                         ]) {

            player.worldCol = player.worldCol - player.cdel;

        }
        player.aniState = LEFT;
    }
    if ((~((*(volatile unsigned short *)0x04000130)) & ((1<<4)))) {
        if (player.worldCol + player.width + 1 <= 240
            &&collisionmap[((player.worldRow - (int) round(player.velocity * 0.1))*(240)+(player.worldCol + player.width))
                                                                         ]
            && collisionmap[((player.worldRow + player.height - 2 - (int) round(player.velocity * 0.1))*(240)+(player.worldCol + player.width))
                                                                         ]
            && collisionmap[((player.worldRow + player.height / 2 - 1 - (int) round(player.velocity * 0.1))*(240)+(player.worldCol + player.width))
                                                                         ]) {

            player.worldCol = player.worldCol + player.cdel;

        }
        player.aniState = RIGHT;
    }
    if ((~((*(volatile unsigned short *)0x04000130)) & ((1<<0))) && canJump) {
        player.worldRow -= 5;
  player.velocity += 40;

        player.jumpTimer = 0;

    }
    else if (player.jumpTimer < 6 && (!(~(oldButtons)&((1<<0))) && (~buttons & ((1<<0)))) && !canJump && player.velocity > 0) {
        player.velocity += 10;
        player.jumpTimer++;
    }

    animatePlayer();
}


void animatePlayer() {


    player.prevAniState = player.aniState;
    player.aniState = IDLE;


    if(player.aniCounter % 20 == 0) {
        player.curFrame = (player.curFrame + 1) % player.numFrames;
    }


    if((~((*(volatile unsigned short *)0x04000130)) & ((1<<5))))
        player.aniState = LEFT;
    if((~((*(volatile unsigned short *)0x04000130)) & ((1<<4))))
        player.aniState = RIGHT;


    if (player.aniState == IDLE) {
        player.curFrame = 0;
        player.aniCounter = 0;
        player.aniState = player.prevAniState;
    } else {
        player.aniCounter++;
    }
}


